{
  "name": "cardsforhumanity",
  "description": "A Game For Horrible People Desperately Trying To Do Good",
  "version": "1.0.0",
  "private": false,
  "author": "cardsforhumanity",
  "repository": {
    "type": "git",
    "url": "https://github.com/cardsforhumanity/cardsforhumanity.git"
  },
  "engines": {
    "node": "6.10.2",
    "npm": "3.10.10"
  },
  "scripts": {
    "preinstall": "npm prune",
    "postinstall": "chmod +x ./update-commit-hooks.sh && ./update-commit-hooks.sh",
    "commit-hook": "rsync pre-commit .git/hooks && chmod 755 .git/hooks/pre-commit",
    "deploy": "node node_modules/bower/bin/bower install && node node_modules/forever/bin/forever -m 5 server.js",
    "start": "gulp",
    "test:dev": "gulp test-dev",
    "test": "gulp test"
