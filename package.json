    "express": "~3.4.0",
    "express-session": "^1.15.2",
    "forever": "latest",
    "gulp-bower": "0.0.13",
    "gulp-coveralls": "^0.1.4",
    "gulp-eslint": "^3.0.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-util": "^3.0.8",
    "jade": "~0.35.0",
